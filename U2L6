def linsearch(arr):
  a = []
  for i in range (len(arr)):
    if (arr[i] == 20) or (arr[i] == 25) or (arr[i] == 60) or (arr[i] == 74) or (arr[i] == 97):
      a.append(i)
  return(a)
 
myArr = [20, 20, 24, 27, 39, 40, 43, 46, 50, 
         60, 60, 62, 71, 74, 76, 86, 86, 87, 97, 97]

print(linsearch(myArr))
